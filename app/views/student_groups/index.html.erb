<p id="notice"><%= notice %></p>

<h3><%=t('views.students_group.listing_student_groups')%></h3>

<table class="table">
  <thead>
    <tr>
      <th><%=t('views.students_group.name')%></th>
      <th><%=t('views.students_group.year')%></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_groups.each do |student_group| %>
      <tr>
        <td><%= student_group.name %></td>
        <td><%= student_group.year %></td>
        <td><%= link_to t('show'), student_group %></td>
        <td><%= link_to t('edit'), edit_student_group_path(student_group) %></td>
        <td><%= link_to t('destroy'), student_group, method: :delete, data: { confirm: t('are_you_sure') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('views.students_group.new_student_group'), new_student_group_path %>
