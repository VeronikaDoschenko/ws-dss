<p id="notice"><%= notice %></p>

<h1><%=t('views.ws_model_statuses.list_of_ws_model_statuses')%></h1>

<table class="table">
  <thead>
    <tr>
      <th><%=t('views.ws_model_statuses.name')%></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ws_model_statuses.each do |ws_model_status| %>
      <tr>
        <td><%= ws_model_status.name %></td>
        <td><%= link_to t('show'), ws_model_status %></td>
        <td><%= link_to t('edit'), edit_ws_model_status_path(ws_model_status) %></td>
        <td><%= link_to t('destroy'), ws_model_status, method: :delete, data: { confirm: t('are_you_sure') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('views.ws_model_statuses.new_ws_model_status'), new_ws_model_status_path %>
