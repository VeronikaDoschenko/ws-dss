<p id="notice"><%= notice %></p>

<h1>Listing Ws Model Statuses</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ws_model_statuses.each do |ws_model_status| %>
      <tr>
        <td><%= ws_model_status.name %></td>
        <td><%= link_to 'Show', ws_model_status %></td>
        <td><%= link_to 'Edit', edit_ws_model_status_path(ws_model_status) %></td>
        <td><%= link_to 'Destroy', ws_model_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ws model status', new_ws_model_status_path %>
