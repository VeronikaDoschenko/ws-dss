<h1><%=t('views.ws_set_model_runs.list_of_ws_set_model_runs')%></h1>

<table class="table">
  <thead>
    <tr>
      <th><%=t('views.ws_set_model_runs.name')%></th>
      <th><%=t('views.ws_set_model_runs.descr')%></th>
      <th><%=t('views.ws_set_model_runs.capacity')%></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ws_set_model_runs.each do |ws_set_model_run| %>
      <tr>
        <td><%= ws_set_model_run.name %></td>
        <td><%= ws_set_model_run.descr %></td>
        <td><%= ws_set_model_run.ws_model_runs.count %></td>
        <td><%= link_to ws_set_model_run, class: "btn btn-default", title: t('show') do %>
              <i class="glyphicon glyphicon-zoom-in"></i>
            <% end %>
        </td>
        <td>
          <% if can? :update, ws_set_model_run %>    
            <%= link_to edit_ws_set_model_run_path(ws_set_model_run), class: "btn btn-default", title: t('edit') do %>
              <i class="glyphicon glyphicon-pencil"></i>
            <% end %>
          <% end %>
        </td>
        <td>
          <% if can? :destroy, ws_set_model_run %> 
            <%= link_to ws_set_model_run, class: "btn btn-default", method: :delete, title: t('delete'), data: { confirm: t('are_ypu_sure') } do %>
                <i class="glyphicon glyphicon-trash"></i>
            <% end %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('views.ws_set_model_runs.new_ws_set_model_runs'), new_ws_set_model_run_path %>
